<head>
    <style>
        .Container {
            padding-left: 20;
        }

        table,th,td {
            border: 2px solid black
        }
    </style>
</head>

<body>
    <div class="Container">
        <h1>Employees</h1>
        <table>
            <tr>
                <th>EID</th>
                <th>Name</th>
                <th>Role</th>
                <th>Salary</th>
                <th>Update</th>
            </tr>
            <% for(let e of employees) {%>
                <tr>
                    <td><%= e.eid %></td>
                    <td><%= e.ename %></td>
                    <td><%= e.role %></td>
                    <td><%= e.salary %></td>
                    <td><a href="/employees/edit/<%= e.eid %>">Update</a></td>
                </tr>
                <% }%>
        </table>
        <br>
        <a href="/">Home</a>
    </div>
</body>